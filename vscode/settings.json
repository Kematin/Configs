{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Tokyo Storm AltSynt",
    "[python]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "codeAction.fixViolation.enable": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.formatOnType": true
    },
    "window.zoomLevel": 1,
    "files.autoSave": "onWindowChange",
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "workbench.list.smoothScrolling": true,
    "emmet.includeLanguages": {
        "django-html": "html"
    },
    "errorLens.excludeBySource": [
        "Flake8(E501)",
        "ts(1128)"
    ],
    "vim.insertModeKeyBindingsNonRecursive": [
        {
            "before": ["j", "k"],
            "after": ["esc"]
        },
        {
            "before": ["<C-n>"],
            "commands": [":nohl"]
        }
    ],
    "editor.semanticTokenColorCustomizations": {
        "enabled": true
    },
    "indentRainbow.includedLanguages": ["ts", "js", "html", "css", "django-html"],
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.lightIndicatorStyleLineWidth": 3,
    "explorer.confirmDelete": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "[javascript][javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
        "editor.tabSize": 2,
        "editor.indentSize": 2
    },
    "[django-html][css][html]": {
        "editor.tabSize": 2
    },
    "terminal.integrated.commandsToSkipShell": [
        "terminalTabs.createTerminal"
    ],
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.startupEditor": "none",
    "python.condaPath": "/Users/kematin/micromamba/condabin/conda",
    "editor.fontSize": 16,
    "terminal.integrated.env.osx": {},
    "terminal.integrated.env.windows": {},
    "security.workspace.trust.untrustedFiles": "open",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.openRepositoryInParentFolders": "always",
    "redhat.telemetry.enabled": false,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Ubuntu-22.04 (WSL)": {
            "path": "C:\\Windows\\System32\\wsl.exe",
            "args": [
                "-d",
                "Ubuntu-22.04"
            ]
        }
    },
    "terminal.integrated.defaultProfile.windows": "Ubuntu-22.04 (WSL)",
    "terminal.integrated.env.linux": {},
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-linux": "/home/kematin/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.minikube-path-linux": "/home/kematin/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vscode-kubernetes.kubectl-path-linux": "/home/kematin/.local/state/vs-kubernetes/tools/kubectl/kubectl"
    },
    "remote.autoForwardPortsSource": "hybrid",
    "codeium.enableCodeLens": false
}
